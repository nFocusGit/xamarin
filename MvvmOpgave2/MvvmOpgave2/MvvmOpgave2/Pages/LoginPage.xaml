<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MvvmOpgave2.Pages.LoginPage">

  <StackLayout >
    <Label x:Name="versionsLabel" Text="{Binding VersionsText}" HorizontalOptions="Center" />
    <Label x:Name="statusLabel" Text="{Binding StatusText}" HorizontalOptions="Center" />
    <Entry x:Name="loginEntry" Placeholder="1234" Keyboard="Numeric" />
    <Button x:Name="okButton" Text="OK"/>
  </StackLayout>  
  
</ContentPage>